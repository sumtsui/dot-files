{
  "editor.wordWrap": "on",
  "editor.fontSize": 15,
  "workbench.colorCustomizations": {},
  "json.format.enable": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.visible": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact",
    "wxml": "html"
  },
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.cursorBlinking": "phase",
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": true
  },
  "editor.quickSuggestionsDelay": 100,
  "editor.parameterHints.enabled": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "terminal.integrated.fontSize": 14,
  "editor.minimap.enabled": false,
  "typescript.validate.enable": true,
  "javascript.validate.enable": true,
  "editor.formatOnPaste": true,
  "terminal.external.osxExec": "$SHELL",
  "javascript.preferences.quoteStyle": "single",
  "editor.tabSize": 2,
  "[json]": {},
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.enablePreview": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "disabled",
  "java.configuration.checkProjectSettingsExclusions": false,
  "gitlens.codeLens.enabled": false,
  "typescript.tsserver.log": "off",
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "prettier.requireConfig": true,
  "explorer.confirmDragAndDrop": false,
  "search.exclude": {
    "node_modules/": true,
    "dist/": true,
    ".cache/": true
  },
  "search-node-modules.useLastFolder": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.associations": {
    "*.properties": "log",
    "*.cjson": "jsonc",
    "*.wxss": "css",
    "*.wxs": "javascript"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "java.semanticHighlighting.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "java.project.importOnFirstTimeStartup": "automatic",
  "editor.formatOnSave": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "workbench.tips.enabled": false,
  "npm-intellisense.recursivePackageJsonLookup": false,
  "path-intellisense.autoSlashAfterDirectory": true,
  "terminal.integrated.tabs.enabled": false,
  "workbench.colorTheme": "Aquatic blue",
  "javascript.suggest.completeJSDocs": false,
  "minapp-vscode.disableAutoConfig": true,
  "editor.snippetSuggestions": "bottom",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "breadcrumbs.enabled": false,
  "window.zoomLevel": -1,
  "settingsSync.ignoredSettings": [],
  "security.workspace.trust.untrustedFiles": "open"
}
